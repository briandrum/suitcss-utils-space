{
  "name": "suitcss-utils-space",
  "version": "0.1.0",
  "description": "Spacing utilities for SUIT CSS",
  "keywords": [
    "browser",
    "css-utilities",
    "space",
    "suitcss",
    "style"
  ],
  "homepage": "https://github.com/briandrum/utils-space#readme",
  "bugs": "https://github.com/briandrum/utils-space/labels/bug",
  "license": "MIT",
  "author": "Brian Drum",
  "files": [
    "index.css",
    "index.js",
    "lib"
  ],
  "style": "index.css",
  "repository": "briandrum/utils-space",
  "scripts": {
    "build": "npm i && npm run preprocess",
    "build-test": "npm i && npm run preprocess-test",
    "lint": "suitcss index.css build/lint.css",
    "preprocess": "suitcss index.css build/build.css",
    "preprocess-test": "suitcss -i test test/test.css build/test.css",
    "watch": "npm run preprocess-test -- -w -v",
    "test": "npm run lint"
  },
  "devDependencies": {
    "suitcss-components-test": "*",
    "suitcss-preprocessor": "^4.0.0"
  }
}
